<Window x:Class="MasterFloor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MasterFloor"
        mc:Ignorable="d"
        Title="Система управления партнерами" Height="450" Width="800"  MinWidth="600" FontFamily="Segoe UI" Background="#F4E8D3">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
           <Image Source="/Images/Мастер пол.png" HorizontalAlignment="Left" Margin="5"/>
            <TextBlock Text="Мастер пол" FontSize="30" VerticalAlignment="Center" Foreground="#67BA80"/>
        </StackPanel>
        <Frame Grid.Row="1" Name="MainFrame"/>
    </Grid>
</Window>
